<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="edu.umsl.bekvff.contactslist.MainActivity">
    <FrameLayout
        android:id="@+id/frame_for_recycle_view"
        android:layout_width="match_parent"
        android:layout_height="453dp"/>
    <FrameLayout
        android:id="@+id/frame_for_create_contact_button"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/frame_for_recycle_view"
        android:layout_alignParentBottom="true">
        <ImageButton
            android:layout_height="55sp"
            android:layout_width="55sp"
            android:id="@+id/create_contact_button"
            android:onClick="createContactButtonClicked"
            android:layout_marginRight="10dp"
            android:layout_gravity="right" />
    </FrameLayout>
</RelativeLayout>
